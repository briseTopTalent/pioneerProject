{
	"name": "firewire",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"cli": "node ./server/src/cli.js",
		"dev": "nodemon server/src/index.js",
		"build": "DISABLE_ESLINT_PLUGIN=true next build",
		"start": "NODE_ENV=development node server/src/index.js",
		"dev:next": "next dev",
		"build:next": "next build",
		"start:next": "next start",
		"lint": "next lint",
		"check-types": "tsc --pretty --noEmit",
		"check-format": "prettier --check .",
		"check-lint": "eslint . --ext tsx --ext jsx --fix",
		"format": "prettier --write .",
		"test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
		"test": "PIONEER_ENV_FILE=$PWD/.env.dev npx jest",
		"heroku-prebuild": "NODE_ENV=development npm install --force && npm run build",
		"prepare": "husky install",
		"make": "cp _tsconfig.json tsconfig.json ; ./make.sh",
		"lint-server": "find server/src/ -type f -path '*.js' -exec node -c {} \\;"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"next/babel"
		]
	},
	"dependencies": {
		"@mentoc/xtract": "^1.0.3",
		"@react-oauth/google": "^0.11.1",
		"@types/express": "^4.17.18",
		"@types/node": "^20.8.4",
		"await-to-js": "^3.0.0",
		"aws-sdk": "^2.1317.0",
		"axios": "^0.26.0",
		"bcryptjs": "^2.4.3",
		"celebrate": "^15.0.1",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"crypto": "^1.0.1",
		"dotenv": "^16.0.0",
		"express": "^4.17.3",
		"express-request-id": "^2.0.1",
		"fb": "^2.0.0",
		"form-data": "^4.0.0",
		"formik": "^2.2.9",
		"glob": "^7.2.0",
		"google-auth-library": "^9.1.0",
		"google-map-react": "^2.1.10",
		"helmet": "^5.0.2",
		"js-cookie": "^3.0.1",
		"jsonwebtoken": "^8.5.1",
		"jwks-rsa": "^3.1.0",
		"mailgun.js": "^8.0.1",
		"moment": "^2.29.1",
		"morgan": "^1.10.0",
		"next": "12.1.0",
		"next-cookies": "^2.0.3",
		"nodemon": "^2.0.15",
		"pg": "^8.7.3",
		"prop-types": "^15.7.2",
		"query-string": "^7.1.1",
		"react": "17.0.2",
		"react-alert": "^7.0.3",
		"react-alert-template-basic": "^1.0.2",
		"react-dom": "17.0.2",
		"react-feather": "^2.0.9",
		"react-paginate": "^8.1.2",
		"react-select": "^5.2.2",
		"react-toastify": "^8.2.0",
		"redis": "^4.6.10",
		"save": "^2.9.0",
		"sequelize": "^5.22.5",
		"twitter": "^1.7.1",
		"twitter-api-v2": "^1.15.2",
		"winston": "^3.6.0",
		"yup": "^0.32.11"
	},
	"devDependencies": {
		"@babel/core": "^7.15.0",
		"@babel/eslint-parser": "^7.15.0",
		"@babel/plugin-syntax-jsx": "^7.16.7",
		"@babel/plugin-transform-runtime": "^7.15.0",
		"@next/eslint-plugin-next": "^11.1.2",
		"@types/fb": "^0.0.28",
		"@types/jest": "^29.5.5",
		"@types/react": "^18.2.28",
		"babel-eslint": "^10.1.0",
		"babel-plugin-jsx": "^1.2.0",
		"babel-plugin-styled-components": "^1.13.2",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-next": "12.1.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.20.0",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.24.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"husky": "^7.0.0",
		"jest": "^29.7.0",
		"lint-staged": "^11.1.2",
		"next-offline": "^5.0.5",
		"parse-address": "^1.1.2",
		"prettier": "2.4.1",
		"serve": "^14.2.1",
		"ts-jest": "^29.1.1"
	}
}
